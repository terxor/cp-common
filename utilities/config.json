{
  "langs" : [
    {
      "name": "C++",
      "ext": "cc",
      "commands": {
        "compile": "g++ %.cc -o % -std=c++17 -O2 -Wall -Wextra -DLOCAL",
        "run" : "%"
      }
    },
    {
      "name": "Java",
      "ext": "java",
      "commands": {
        "compile": "javac %.java",
        "run" : "java %"
      }
    },
    {
      "name": "Python",
      "ext": "py",
      "commands": {
        "run" : "python %.py"
      }
    }
  ],
  "runConfig": {
    "terminalIO" : true,
    "inputFile" : "input.txt",
    "outputFile" : "output.txt",
    "useSameDir": true,
    "timeLimit": 2
  },
  "useColors" : false
}
